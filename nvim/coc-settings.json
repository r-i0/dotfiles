{
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"],
    }
  },
  "cSpell.userWords": [
    "Chakra",
    "Fdump",
    "Teee",
    "datings",
    "hoge",
    "recruitings"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "markdown",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript",
    "json",
    "graphql"
  ]
}
